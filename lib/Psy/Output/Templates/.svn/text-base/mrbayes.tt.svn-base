#Nexus
[ MrBayes input file generated by Psy ver something. ]

Begin data;

Dimensions
	ntax = [% total_ters %]
	nchar = [% total_chars %];
Format 
	data = DNA
	missing = ?
	gap = - 
	;

matrix
[% mask_head_aligner %][% FOREACH blk = unbracketed %][% blk_mask(blk) %][% END %]]
[% FOREACH ter = terminals %][% justifiedrowlabel(ter) %][% FOREACH blk = unbracketed %][% cell(ter, blk) %][% END %]
[% END %];
END;

[the Bayes block]
[ REVIEW THESE SETTINGS CAREFULLY, THEY ARE NOT INTENDED TO DEFINE A PUBLICATION WORTHY ANALYSIS ]
begin mrbayes;
[ uncomment following two lines for parallel runs ]
[ set autoclose=yes; ]
[ set nowarnings=yes; ]
[ set quitonerror =yes; ]
	
[charsets]

[ charsets loops here]
pairs [% nucpairs %];
charset non_stems = [% unpaired_positions %];
charset stems = [% paired_positions %];

partition allrna = 2: non_stems, stems;
set partition = allrna;

[basic model information]

lset applyto=(2) nucmodel=doublet; [must be in for the stem model]

[lset applyto=(1) rates=invgamma ngammacat=4 nst=1 or 2 or 6;]
[lset applyto=(2) rates=invgamma ngammacat=4 nst=6;]
[lset applyto=(3) rates=invgamma ngammacat=4 nst=6;]
		
unlink shape=(all) Tratio=(all) pinvar=(all) statefreq=(all) revmat=(all);

[mcmc settings]
mcmcp 
Nchains=6
seed=[% randseed %]
swapseed=[% randseed %]
stoprule=yes
stopval=0.05
burninfrac=0.25
Ngen=3000000 
Samplefreq=1000
Printfreq=10000 
Burnin=400000
Reweight=(0.00,0.00)
Startingtree=Random
Nperts=0
Savebrlens=Yes
[Filename=foo]
;
		
end;
		
