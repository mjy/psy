[the Bayes block]
begin mrbayes;
[ set autoclose=yes; ]
[ set nowarnings=yes; ]
	
[charsets]

[ charsets loops here]
pairs 1:40, 2:39, 4:38, 6:35, 7:34, 10:33, 11:28, 12:27, 13:26, 14:25, 16:24, 17:23, 18:22, 19:21, 29:43, 30:42, 32:41;
charset non_stems = 3 5 8 9 15 20 31 36 37;
charset stems = 1 40 2 39 4 38 6 35 7 34 10 33 11 28 12 27 13 26 14 25 16 24 17 23 18 22 19 21 29 43 30 42 32 41;

partition allrna = 2: non_stems, stems;
set partition = allrna;

[basic model information]

lset applyto=(2) nucmodel=doublet; [must be in for the stem model]

[lset applyto=(1) rates=invgamma ngammacat=4 nst=1 or 2 or 6;]
[lset applyto=(2) rates=invgamma ngammacat=4 nst=6;]
[lset applyto=(3) rates=invgamma ngammacat=4 nst=6;]
		
unlink shape=(all) Tratio=(all) pinvar=(all) statefreq=(all) revmat=(all);

[mcmc settings]
mcmcp 
seed=2
Ngen=2000000 
Samplefreq=100
Printfreq=1000 
Burnin=40000
Nchains=10
Temp=0.200000
Reweight=(0.00,0.00)
[Filename=foo]

Startingtree=Random
Nperts=0
Savebrlens=Yes
;
		
end;