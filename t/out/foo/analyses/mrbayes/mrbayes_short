[the Bayes block]
begin mrbayes;
[ set autoclose=yes; ]
[ set nowarnings=yes; ]
	
[charsets]

[ charsets loops here]
pairs ;
charset non_stems = ;
charset stems = ;

partition allrna = 2: non_stems, stems;
set partition = allrna;

[basic model information]

lset applyto=(2) nucmodel=doublet; [must be in for the stem model]

[lset applyto=(1) rates=invgamma ngammacat=4 nst=1 or 2 or 6;]
[lset applyto=(2) rates=invgamma ngammacat=4 nst=6;]
[lset applyto=(3) rates=invgamma ngammacat=4 nst=6;]
		
unlink shape=(all) Tratio=(all) pinvar=(all) statefreq=(all) revmat=(all);

[mcmc settings]
mcmcp 
seed=2
Ngen=2000000 
Samplefreq=100
Printfreq=1000 
Burnin=40000
Nchains=10
